# ============================================================================
# ComfyUI React - Environment Configuration Example
# ============================================================================
# Copy this file to .env.local for local development overrides

# API Configuration
VITE_API_URL=http://127.0.0.1:8188
VITE_WS_URL=ws://127.0.0.1:8188/ws

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_MOCK_API=false
VITE_ENABLE_ANALYTICS=false

# Development Settings
VITE_LOG_LEVEL=debug
VITE_CACHE_DURATION=0

# Build Configuration
VITE_BUILD_TARGET=development

# Optional: Custom Configuration
# VITE_CUSTOM_THEME=dark
# VITE_MAX_HISTORY_ITEMS=100
# VITE_AUTO_SAVE_INTERVAL=30000